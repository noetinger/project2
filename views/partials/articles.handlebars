
{{#if articles}}
    {{#each articles}}
    {{log this.title}}
        <li data-id="articleTitle" class="list-group-item">{{this.title}}</li>
        <li data-id="articleLink" class="list-group-item">{{this.link}}</li>
    {{/each}}
{{else}}
    <li>Sorry, we couldn't find any articles!</li>
{{/if}}

