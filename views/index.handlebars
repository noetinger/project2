<div id="home">
  <div class="container main-stuff">
    <div class="row">
      <div class="col-8 offset-2">
        <div class="card">
          <div class="card-body"></div>
          <h1>Welcome to Crafty Drafter!</h1>
          {{!-- <h2>This is our Home Page!</h2> --}}
          <form class="clearfix mb-4" action="POST">
            <div class="form-group">
              <select>
                <option value="type">Beer Type</option>
                <option value="breweryName">Brewery Name</option>
              </select>
              <input type="text" id="searchTerm" class="form-control" aria-describedby="example-text"
                placeholder="Enter Search Here">
              <button id="searchBtn" class="btn btn-primary float-right">Submit</button>
          </form>
          <h3>Examples:</h3>
          <ul id="example-list" class="list-group">
            {{#each examples}}
            <li data-id="{{this.id}}" class="list-group-item">
              <a href="example/{{this.id}}">{{this.text}}</a>
              <button class="btn btn-danger float-right delete">ï½˜</button>
            </li>
            {{/each}}
          </ul>
        </div>
      </div>\
    </div>
  </div>
</div>
</div>






<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>